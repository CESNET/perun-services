#!/usr/bin/env python3

# this script sends data generated by the `generic_json_gen` gen script. Name might be confusing, but services store
# only one script name, which is then the same for gen/send

import sys

import generic_sender
import send_lib

send_lib.check_input_fields(sys.argv, generic_script=True)

facility = sys.argv[1]
destination = sys.argv[2]
destination_type = sys.argv[3]
service_name = sys.argv[4]

generic_sender.send(service_name, facility, destination, destination_type)
