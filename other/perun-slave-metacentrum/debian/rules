#!/usr/bin/make -f
# -*- makefile -*-
# Sample debian/rules that uses debhelper.
# This file was originally written by Joey Hess and Craig Small.
# As a special exception, when this file is copied by dh-make into a
# dh-make output file, you may use that output file without restriction.
# This special exception was added by Craig Small in version 0.37 of dh-make.

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh $@ 

override_dh_gencontrol:
	dh_gencontrol -- -Vmisc:Depends="perun-slave-process-afs, perun-slave-process-afs-group, perun-slave-process-apache-basic-auth, perun-slave-process-apache-ssl, perun-slave-process-fs-home, perun-slave-process-fs-project, perun-slave-process-fs-quotas, perun-slave-process-fs-scratch, perun-slave-process-fs-scratch-local, perun-slave-process-group, perun-slave-process-group-nfs4, perun-slave-process-k5login, perun-slave-process-k5login-generic, perun-slave-process-k5login-root, perun-slave-process-mailaliases, perun-slave-process-mailaliases-generic, perun-slave-process-mailman-meta, perun-slave-process-passwd, perun-slave-process-passwd-nfs4, perun-slave-process-passwd-scp, perun-slave-process-pbsmon-json, perun-slave-process-pbsmon-users, perun-slave-process-pbs-publication-fairshare, perun-slave-process-pkinit, perun-slave-process-sshkeys"
